<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="VerifyEmployeesStatus"
    targetNamespace="http://enterprise.netbeans.org/bpel/esb/VerifyEmployeeStatus"
    xmlns:tns="http://enterprise.netbeans.org/bpel/esb/VerifyEmployeeStatus"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" location="VerifyEmployeesStatus.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/SkillsVerifyEmployeesStatusWrapper" location="SkillsVerifyEmployeesStatusWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services/" location="http://localhost:30687/interop-mock/VerifyEmployeesStatus?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="SkillsVerifyPartnerLink" xmlns:tns="http://enterprise.netbeans.org/bpel/SkillsVerifyEmployeesStatusWrapper" partnerLinkType="tns:SkillsVerifyEmployeesStatusLinkType" partnerRole="SkillsVerifyEmployeesStatusRole"/>
        <partnerLink name="BpelVerifyPartnerLink" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" partnerLinkType="tns:VerifyEmployeesStatus" myRole="VerifyEmployeesStatusPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="VerifyEmployeesStatusOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" messageType="tns:VerifyEmployeesStatusOperationRequest"/>
        <variable name="VerifyEmployeesStatusOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" messageType="tns:VerifyEmployeesStatusOperationResponse"/>
        <variable name="SkillsVerifyEmployeesOut" xmlns:tns="http://services/" messageType="tns:verifyEmployeesResponse"/>
        <variable name="SkillsVerifyEmployeesIn" xmlns:tns="http://services/" messageType="tns:verifyEmployees"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="BpelVerifyPartnerLink" operation="VerifyEmployeesStatusOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" portType="tns:VerifyEmployeesStatusPortType" variable="VerifyEmployeesStatusOperationIn"/>
        <assign name="Assign2">
            <copy>
                <from>$VerifyEmployeesStatusOperationIn.part1/employees</from>
                <to>$SkillsVerifyEmployeesIn.parameters/request/employees</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="SkillsVerifyPartnerLink" operation="verifyEmployees" xmlns:tns="http://services/" portType="tns:VerifyEmployeesStatus" inputVariable="SkillsVerifyEmployeesIn" outputVariable="SkillsVerifyEmployeesOut"/>
        <assign name="Assign1">
            <copy>
                <from>$SkillsVerifyEmployeesOut.parameters/return/results</from>
                <to>$VerifyEmployeesStatusOperationOut.response/results</to>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="BpelVerifyPartnerLink" operation="VerifyEmployeesStatusOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/VerifyEmployeesStatus" portType="tns:VerifyEmployeesStatusPortType" variable="VerifyEmployeesStatusOperationOut"/>
    </sequence>
</process>
