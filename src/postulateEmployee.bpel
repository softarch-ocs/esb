<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="postulateEmployee"
    targetNamespace="http://enterprise.netbeans.org/bpel/esb/esb"
    xmlns:tns="http://enterprise.netbeans.org/bpel/esb/esb"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling"> 
    <import namespace="http://services/" location="http://localhost:28080/interop-mock/PostulateEmployee?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services/" location="http://localhost:28080/interop-mock/VerifyEmployeesStatus?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/HealthPostulateEmployeeWrapper" location="HealthPostulateEmployeeWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services/" location="http://localhost:8080/interop-mock/PostulateEmployee?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/HealthVerifyEmployeesStatusWrapper" location="HealthVerifyEmployeesStatusWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services/" location="http://localhost:8080/interop-mock/VerifyEmployeesStatus?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/SkillsPostulateEmployeeWrapper" location="SkillsPostulateEmployeeWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" location="PostulateEmployee.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" partnerLinkType="tns:PostulateEmployee" myRole="PostulateEmployeePortTypeRole"/>
        <partnerLink name="SkillsPostulateEmployee" xmlns:tns="http://enterprise.netbeans.org/bpel/SkillsPostulateEmployeeWrapper" partnerLinkType="tns:PostulateEmployeeLinkType" partnerRole="SkillsPostulateEmployeeRole"/>
        <partnerLink name="HealthPostulateEmployee" xmlns:tns="http://enterprise.netbeans.org/bpel/HealthPostulateEmployeeWrapper" partnerLinkType="tns:PostulateEmployeeLinkType" partnerRole="HealthPostulateEmployeeRole"/>
    </partnerLinks>
    <variables>
        <variable name="PostulateEmployeeOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" messageType="tns:PostulateEmployeeOperationResponse"/>
        <variable name="SkillsPostulateEmployeeOut" xmlns:tns="http://services/" messageType="tns:postulateResponse"/>
        <variable name="SkillsPostulateEmployeeIn" xmlns:tns="http://services/" messageType="tns:postulate"/>
        <variable name="PostulateEmployeeOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" messageType="tns:PostulateEmployeeOperationRequest"/>
        <variable name="SkillsVerifyEmployeesOut" xmlns:tns="http://services/" messageType="tns:verifyEmployeesResponse"/>
        <variable name="SkillsVerifyEmployeesIn" xmlns:tns="http://services/" messageType="tns:verifyEmployees"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="PostulateEmployeeOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" portType="tns:PostulateEmployeePortType" variable="PostulateEmployeeOperationIn"/>
        <invoke name="Invoke1" partnerLink="SkillsPostulateEmployee" operation="postulate" xmlns:tns="http://services/" portType="tns:PostulateEmployee" inputVariable="SkillsPostulateEmployeeIn" outputVariable="SkillsPostulateEmployeeOut"/>
        <assign name="Assign1">
            <copy>
                <from>$SkillsPostulateEmployeeOut.parameters/return/errorMessage</from>
                <to>$PostulateEmployeeOperationOut.response/errorMessage</to>
            </copy>
            <copy>
                <from>$SkillsPostulateEmployeeOut.parameters/return/success</from>
                <to>$PostulateEmployeeOperationOut.response/success</to>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="PostulateEmployeeOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/esb/src/PostulateEmployee" portType="tns:PostulateEmployeePortType" variable="PostulateEmployeeOperationOut"/>
    </sequence>
</process>
